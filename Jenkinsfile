pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                echo 'Building'
                sleep 1
            }
        }
        stage('Test'){
            steps{
                echo 'Testing'
            }
        }
        stage('Deploy-Staging'){
            steps{
                echo 'Deploy-Staging'
            }
        }
        
        stage('Deploy-Production'){
            steps{
                echo 'Deploy-Production'
            }
        }
    }
}
