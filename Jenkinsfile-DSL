pipeline{
    agent any
    stages{
         
        stage("Build"){
            steps{
                timeout(time:20, unit:'MINUTES'){
                    script{
                        alauda.startBuild("testorg001", "")
                    }
                }
            }
        }

    }
 
    post{
        always{
            script{
                alauda.notify("yzwang1")
            }
            
        }
    }
}
