pipeline{
    agent any
    stages{
         
        stage("Build"){
            timeout(time:20, unit:'MINUTES'){
                script{
                    alauda.startBuild("testorg001", "${GIT_COMMIT_ID}")
                }
                
            }
        }

    }
 
    post{
        always{
            script{
                alauda.notify("yzwang1")
            }
            
        }
    }
}
